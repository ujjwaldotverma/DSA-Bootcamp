import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Even or Odd");
      Scanner input= new Scanner(System.in);
      int a = input.nextInt();
      if(a%2==0)
        System.out.println(a+ " is even");
      else
        System.out.println(a+ " is odd");
    }
}
