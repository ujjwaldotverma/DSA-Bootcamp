import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("PROGRAM TO ADD 2 NUMBERS");
      Scanner input= new Scanner(System.in);
      int a = input.nextInt();
      int b = input.nextInt();
      int c;
      c = a + b;
      System.out.println(c);
      
    }
}
